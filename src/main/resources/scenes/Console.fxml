<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="435.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.ControllerConsole">
    <children>
        <TextArea fx:id="consoleArea" editable="false" prefHeight="400.0" prefWidth="600.0" wrapText="true">
            <font>
                <Font name="Lucida Console" size="14.0"/>
            </font>
        </TextArea>
        <Button fx:id="saveButton" layoutX="500.0" layoutY="400.0" mnemonicParsing="false" onAction="#onSave" prefHeight="35.0"
                prefWidth="100.0" text="Save"/>
    </children>
</AnchorPane>
