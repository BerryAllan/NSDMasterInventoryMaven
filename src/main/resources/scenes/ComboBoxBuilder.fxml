<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.ControllerComboBoxBuilder">
    <Button fx:id="addButton" layoutX="90.0" layoutY="94.0" mnemonicParsing="false" onAction="#onAddComboBoxItem" text="Add"/>
    <TextField fx:id="textField" onAction="#onAddComboBoxItem" layoutX="21.0" layoutY="31.0"/>
    <Button fx:id="editButton" layoutX="92.0" layoutY="185.0" mnemonicParsing="false" onAction="#onEditComboBoxItem" text="Edit"/>
    <Button fx:id="removeButton" layoutX="77.0" layoutY="279.0" mnemonicParsing="false" onAction="#onRemoveComboBoxItem" text="Remove"/>
    <Button fx:id="saveButton" layoutX="14.0" layoutY="349.0" mnemonicParsing="false" onAction="#saveButtonAction" text="Save">
        <font>
            <Font size="16.0"/>
        </font>
    </Button>
    <ListView fx:id="listView" layoutX="250.0" prefHeight="400.0" prefWidth="350.0"/>
</AnchorPane>
