<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.ControllerItemManager">
    <Button fx:id="add" layoutX="118.0" layoutY="130.0" mnemonicParsing="false" onAction="#showItemBuilderWindow" text="Add">
        <font>
            <Font size="24.0"/>
        </font>
    </Button>
    <Button fx:id="edit" layoutX="120.0" layoutY="275.0" mnemonicParsing="false" onAction="#showEditorWindow" text="Edit">
        <font>
            <Font size="24.0"/>
        </font>
    </Button>
    <Button fx:id="remove" layoutX="98.0" layoutY="415.0" mnemonicParsing="false" onAction="#removeItem" text="Remove">
        <font>
            <Font size="24.0"/>
        </font>
    </Button>
    <ListView fx:id="listView" layoutX="300.0" prefHeight="600.0" prefWidth="600.0"/>
</AnchorPane>
